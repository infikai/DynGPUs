user  nginx;
worker_processes  auto;

error_log  /var/log/nginx/error.log notice;
pid        /run/nginx.pid;

events {
    worker_connections  1024;
}

http {
    upstream inference_servers {
        # This block will be filled in by the Python script
{UPSTREAM_SERVERS}
    }

    server {
        listen 8888;

        location / {
            proxy_pass http://inference_servers;
        }
    }
}